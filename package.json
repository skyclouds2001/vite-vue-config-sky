{
  "name": "vite-vue-template-sky",
  "private": true,
  "version": "1.2.0",
  "description": "an opinionated vite starter template for vue",
  "keywords": [
    "template",
    "project",
    "vite",
    "vue",
    "typescript",
    "vue router",
    "pinia",
    "element plus",
    "tailwindcss",
    "iconify",
    "pnpm",
    "eslint",
    "prettier",
    "stylelint",
    "commitlint",
    "markdownlint",
    "lint-staged",
    "husky",
    "vitest",
    "playwright",
    "commitizen",
    "changeset"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyclouds2001/vite-vue-template-sky.git"
  },
  "homepage": "https://github.com/skyclouds2001/vite-vue-template-sky#readme",
  "bugs": {
    "url": "https://github.com/skyclouds2001/vite-vue-template-sky/issues",
    "email": "skyclouds2001@163.com"
  },
  "license": "MIT",
  "author": {
    "name": "skyclouds2001",
    "email": "skyclouds2001@163.com",
    "url": "https://skyclouds2001.github.io/"
  },
  "contributors": [
    "skyclouds2001"
  ],
  "main": "src/main.ts",
  "type": "module",
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=9.6.0",
    "yarn": ">=1.22.0",
    "pnpm": ">=8.4.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint && pnpm lint:markdownlint && pnpm lint:type",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "lint:stylelint": "stylelint --fix",
    "lint:markdownlint": "markdownlint --fix **/*.md",
    "lint:type": "vue-tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "cross-env NODE_ENV=test vitest",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "clean": "rimraf node_modules && rimraf dist",
    "changeset": "changeset",
    "bump": "changeset version",
    "commit": "cz",
    "prepare": "is-ci || husky",
    "preinstall": "only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.4",
    "dayjs": "^1.11.12",
    "element-plus": "^2.8.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify/vue": "^4.1.2",
    "@playwright/test": "^1.46.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.4.1",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "is-ci": "^3.0.1",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.9",
    "markdownlint": "^0.34.0",
    "markdownlint-cli": "^0.41.0",
    "only-allow": "^1.2.1",
    "pnpm": "9.7.1",
    "postcss-import": "^16.1.0",
    "postcss-load-config": "^6.0.1",
    "postcss-preset-env": "^10.0.2",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.8.2",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^5.4.1",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-pwa": "^0.20.1",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  }
}
